package aaa.model;

import java.util.HashMap;

import org.springframework.stereotype.Component;

@Component
public class BanMain {
	
	//2중 배열 느낌
	HashMap<String, String[]>data;
	
	public BanMain() {
		data = new HashMap<>();
		
		data.put("class1","장동건,장서건,장남건".split(","));
		data.put("class2","sua.png,sub.png".split(","));
		data.put("class3","at_1.jpg,at_2.jpg,at_5.jpg,at_6.jpg".split(","));
		data.put("class4","ww.jpg,ee.jpg,sp1.jpg".split(","));
	}
	
	public String [] getPicture(String title) {
		return data.get(title);
	}
}
