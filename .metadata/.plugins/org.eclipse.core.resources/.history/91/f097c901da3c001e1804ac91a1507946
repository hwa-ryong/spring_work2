package aop_p.webToon;

import org.springframework.stereotype.Component;

@Component
public class Rice {
	
	public String Bob1() {
		System.out.println("제육덮밥");
		return "제육덮밥";
	}
	
	public void setBob2() {
		kind = "불고기덮밥";
	}

	@Override
	public String toString() {
		return "Rice [name=" + name + ", kind=" + kind + "]";
	}
	
	
	
}
