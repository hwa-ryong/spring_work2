package aaa.model;

public class Person {
	
	String pname, gender;
	boolean mil;
	
	public Person(String pname, String gender, boolean mil) {
		super();
		this.pname = pname;
		this.gender = gender;
		this.mil = mil;
	}
	
	

	public String getPname() {
		return pname;
	}



	public String getGender() {
		return gender;
	}



	public boolean isMil() {
		return mil;
	}



	@Override
	public String toString() {
		return "Person [pname=" + pname + ", gender=" + gender + ", mil=" + mil + "]";
	}
	
	
}
