package aaa.model;

import java.util.HashMap;

import org.springframework.stereotype.Component;

@Component
public class BanMain {
	
	//2중 배열 느낌
	HashMap<String, String[]>data;
	
	public BanMain() {
		data = new HashMap<>();
		
		data.put("class1","장동건,장서건,장남건".split(","));
		data.put("class2","1화룡,2화룡".split(","));
		data.put("class3","3화룡,4화룡,5화룡,6화룡".split(","));
		data.put("class4","김혁,이혁,박혁".split(","));
	}
	
	public String [] getName(String name) {
		return data.get(name);
	}
}
