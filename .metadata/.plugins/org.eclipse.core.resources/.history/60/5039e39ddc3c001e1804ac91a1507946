package aop_p.webToon;

import org.springframework.stereotype.Component;

@Component
public class Rice {
	
	int total, price;
	
	
	public String Bob1() {
		System.out.println("제육덮밥");
		return "제육덮밥";
	}
	
	public String Bob2() {
		System.out.println("불고기덮밥");
		return "불고기덮밥";
	}
	
	public String GGigae1() {
		System.out.println("김치찌개");
		return "김치찌개";
	}
	
	public String GGigae2() {
		System.out.println("된장찌개");
		return "된장찌개";
	}

}
