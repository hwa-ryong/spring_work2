package aop_p.webToon;

import org.springframework.stereotype.Component;

@Component
public class Stew {
	
	String name, kind;
	
	
	public void setGGigae1() {
		kind = "김치찌개";
	}
	
	public void setGGigae2() {
		kind = "된장찌개";
	}

	@Override
	public String toString() {
		return "Stew [name=" + name + ", kind=" + kind + "]";
	}

	
	
	
	
}
